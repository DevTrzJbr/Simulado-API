// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Paciente {
  id Int     @id @default(autoincrement())
  nome String 
  senha String
  user String @unique
} 

model Secretaria {
  id Int     @id @default(autoincrement())
  nome String
  RG Int      @unique
}

model Consulta {
  id Int     @id @default(autoincrement())
  data DateTime
  nomePaciente String
  nomeDentista String
}

model Agenda {
  id Int     @id @default(autoincrement())
  data DateTime
  nomePaciente String
}